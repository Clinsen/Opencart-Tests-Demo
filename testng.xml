<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="UI Suite" parallel="methods" thread-count="6">
    <listeners>
        <listener class-name="com.demo.tests.base.TestListener"/>
    </listeners>

    <test name="Smoke">
        <packages>
            <package name="com.demo.tests.smoke"/>
        </packages>
    </test>

    <test name="Functional">
        <packages>
            <package name="com.demo.tests.functional"/>
        </packages>
    </test>

    <test name="Regression">
        <packages>
            <package name="com.demo.tests.regression"/>
        </packages>
    </test>
</suite>
